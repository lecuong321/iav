<html>
<body>
<div class="container">
    <strong>Processing...</strong>
    <form action='<%= @url %>' method='post' id='rsessionPost'>
        <input type='hidden' name='rsession' placeholder='User Token' value='<%= @userToken %>' id='rsession'/>
        <input type='hidden' name='app' placeholder='FinappId' value='<%= @finAppId %>' id='finappId'/>
        <input type='hidden' name='redirectReq' placeholder='true/false' value='true'/>
