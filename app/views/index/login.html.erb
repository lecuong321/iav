<div class="panel panel-default col-sm-6 col-sm-offset-3">
  <%= form_tag coblogin_path do %>
  <div class="panel-body">
    <h1>Login Cobrand</h1>
    <input type="hidden" value="coblogin" name="function">
    <div class="form-group">
      <input class="form-control " name="cobrandLogin" placeholder="Enter the cobrand login" value="sbCobhoandinhant">
    </div>
    <div class="form-group">
      <input type="password" class="form-control" name="cobrandPassword" placeholder="Enter the cobrand password" value="0ee26be5-39d2-427d-ae21-421f0b411215">
    </div>
    <div class="form-group">
      <input class="form-control" name="base_url" placeholder="Enter the rest URL" value="https://rest.developer.yodlee.com/services/srest/restserver/v1.0">
    </div>
    <div class="form-group">
      <input class="form-control" name="fastlink_url" placeholder="Fastlink 2.0 URL" value="https://node.developer.yodlee.com/authenticate/restserver/">
    </div>
    <div class="form-group">
      <% flash.each do |name, msg| %>
      <p><font color="red"><%= content_tag :div, msg, :id => "flash_#{name}" %></font></p>
      <% end %>
    </div>
    <button type="submit" class="btn btn-primary pull-right">Submit</button>
  </div>
  <% end %>
</div>
